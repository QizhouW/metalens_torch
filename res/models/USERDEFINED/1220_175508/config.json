{
    "name": "USERDEFINED",
    "n_gpu": 1,
    "arch": {
        "type": "USERDEFINED",
        "args": {}
    },
    "data_loader": {
        "type": "USERDEFINED",
        "args": {
            "data_dir": "data/",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse",
    "metrics": [
        "mae"
    ],
    "lr_scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "patience": 5,
            "factor": 0.2,
            "min_lr": 1e-06
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "res/",
        "save_period": 10,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 8,
        "tensorboard": true
    }
}